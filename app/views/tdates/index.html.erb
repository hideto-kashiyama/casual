<p id="notice"><%= notice %></p>

<h1>Listing Tdates</h1>

<table>
  <thead>
    <tr>
      <th>Dt</th>
      <th>Cap</th>
      <th>Par</th>
      <th>Category</th>
      <th>Product</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tdates.each do |tdate| %>
      <tr>
        <td><%= tdate.dt %></td>
        <td><%= tdate.cap %></td>
        <td><%= tdate.par %></td>
        <td><%= tdate.category_id %></td>
        <td><%= tdate.product_id %></td>
        <td><%= tdate.customer_id %></td>
        <td><%= link_to 'Show', tdate %></td>
        <td><%= link_to 'Edit', edit_tdate_path(tdate) %></td>
        <td><%= link_to 'Destroy', tdate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tdate', new_tdate_path %>
